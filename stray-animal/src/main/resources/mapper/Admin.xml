<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">

<mapper namespace="com.lqa.strayanimal.mapper.AdminMapper">

    <select id="getAdminRoleById" resultMap="com.lqa.strayanimal.entity.RoleEntity">
        select r.*
        from role r,admin_role ar
        where r.id = ar.role_id
        and ar.admin_id = #{adminId}
    </select>
</mapper>
