<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">

<mapper namespace="com.lqa.strayanimal.mapper.AdminRoleMapper">
    <insert id="addRole" parameterType="com.lqa.strayanimal.entity.AdminRoleEntity">
        insert into admin_role (role_id, admin_id) values
        <foreach collection="rolesId" item="role_id" separator=",">
            (#{roleId}, #{adminId})
        </foreach>
    </insert>

</mapper>
