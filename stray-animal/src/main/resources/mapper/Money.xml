<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">

<mapper namespace="com.lqa.strayanimal.mapper.MoneyMapper">

    <select id="getMoneyByPage" resultMap="com.lqa.strayanimal.entity.MoneyEntity">
        select *
        from money
        <if test="page != null and size != null">
            limit #{page}, #{size}
        </if>
    </select>

</mapper>
